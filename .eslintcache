        return false;\n      }\n    })\n  }\n\n  render() {\n\n    const FormSucess = (props) => {\n      /*if(this.formSucess === null) {\n        return <Alert variant=\"danger\">Ocorreu um erro</Alert>\n      } else if(this.formSucess === true) {\n        return <Alert variant=\"primary\">O registro foi adicionado ao com sucesso</Alert>\n      } else if(this.formSucess === false) {\n        return <Alert variant=\"danger\">Ocorreu um erro</Alert>\n      }*/\n      if(props.sucess === true) {\n        return <Alert variant=\"primary\">O registro foi adicionado ao com sucesso</Alert>\n      } else if(props.sucess) {\n        return <Alert variant=\"danger\">Ocorreu um erro</Alert>\n      }\n\n      return <h1></h1>\n    }\n\n\n    return (\n      <div className=\"formAddUser\">\n          <FormSucess sucess={this.state.formSucess}></FormSucess>\n          <Form>\n            <Form.Group>\n              <Form.Label>Nome</Form.Label>\n              <Form.Control type=\"text\" name=\"name\" placeholder=\"Digite seu nome\" value={this.state.name} onChange={this.handleInputChange} />\n            </Form.Group>\n          \n            <Form.Group>\n              <Form.Label>Idade</Form.Label>\n              <Form.Control name=\"age\" type=\"number\" placeholder=\"Digite sua idade\"  value={this.state.age} onChange={this.handleInputChange} />\n            </Form.Group>\n\n            <Form.Group>\n              <Form.Label>Estado Civil</Form.Label>\n              <Form.Control as=\"select\" name=\"civil_state\" defaultValue=\"Solteiro\"  value={this.state.civil_state} onChange={this.handleInputChange}>\n                <option>Solteiro</option>\n                <option>Casado</option>\n                <option>Divorciado</option>\n                <option>Viuvo</option>\n              </Form.Control>\n            </Form.Group>\n              \n            <Form.Group>\n              <Form.Label>CPF</Form.Label>\n              <Form.Control type=\"text\" name=\"CPF\" placeholder=\"Digite seu CPF\" value={this.state.CPF} onChange={this.handleInputChange}/>\n            </Form.Group>\n\n            <Form.Group>\n              <Form.Label>Estado</Form.Label>\n              <Form.Control type=\"text\" name=\"state\" placeholder=\"Digite seu Estado\" value={this.state.state} onChange={this.handleInputChange} />\n            </Form.Group>\n            <Form.Group>\n              <Form.Label>Cidade</Form.Label>\n              <Form.Control type=\"text\" name=\"city\" placeholder=\"Digite sua Cidade\" value={this.state.city} onChange={this.handleInputChange}/>\n            </Form.Group>\n\n\n            <Button variant=\"primary\" type=\"submit\" onClick={this.handleSubmit}>\n              Submit\n            </Button>\n          </Form>\n      </div>\n    );\n  }\n}\n\nexport default FormAddUser;\n","/home/danitw/Development/processo-seletivo/processo-seletivo-frontend/src/components/formEditUser/formEditUser.jsx",["35"],"/home/danitw/Development/processo-seletivo/processo-seletivo-frontend/src/components/modalConfirmDelete/modalConfirmDelete.jsx",["36","37"],"/home/danitw/Development/processo-seletivo/processo-seletivo-frontend/src/components/tableUser/tableUser.jsx",[],{"ruleId":"38","replacedBy":"39"},{"ruleId":"40","replacedBy":"41"},{"ruleId":"42","severity":1,"message":"43","line":81,"column":14,"nodeType":"44","endLine":81,"endColumn":18},{"ruleId":"42","severity":1,"message":"43","line":69,"column":14,"nodeType":"44","endLine":69,"endColumn":18},{"ruleId":"45","severity":1,"message":"46","line":4,"column":8,"nodeType":"47","messageId":"48","endLine":4,"endColumn":13},{"ruleId":"42","severity":1,"message":"43","line":53,"column":14,"nodeType":"44","endLine":53,"endColumn":18},"no-native-reassign",["49"],"no-negated-in-lhs",["50"],"jsx-a11y/heading-has-content","Headings must have content and the content must be accessible by a screen reader.","JSXOpeningElement","no-unused-vars","'Popup' is defined but never used.","Identifier","unuse